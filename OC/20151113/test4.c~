#include <stdio.h>

int main(void)
{
    int s=0;
    int a;
    int size;

    printf("请输入a的值：\n");
    scanf("%d",&a);
    printf("请输入size的值：\n");
    scanf("%d",&size);

    for (int x = 0; x < size; x++)
    {
        int b = a;
        for(int y = 0; y < x; y++ )
        {
            b =b* 10 + a;
        }
        s += b;
    }

    printf("s >>>>>>%d\n",s);

    return 0;

}
