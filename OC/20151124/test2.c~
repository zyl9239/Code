#include <stdio.h>
#include <stdbool.h>
#include <string.h>

int main(void)
{
    char c[15] = "hello,world!";
    char *c1 = "hello,world!";

    for(int i = 0; i < strlen(c); i++)
    {
        printf("%c\t",c[i]);
    }
    printf("\n");

    for(int i = 0; i < strlen(c1); i++)
    {
        printf("%c\t",*(c1+i));
    }
    printf("\n");

    return 0;
}
