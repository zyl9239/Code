#include <stdio.h>
#include <stdbool.h>
#include <string.h>

int main(void)
{
    typedef struct{
        int age;
        float height;
    }STU;

    STU st = {20,1.99};
    STU* stp = &st;
    printf("age >>>> %d\n",stp->age);
    stp->height = 2.26;
    printf("height >>>> %.2f\n",stp->height);

    STU students[10] = {0};
    students->age = 1000;
    printf("students[0]age >>>> %d\n",students[0].age);

    return 0;
}
